{
  "name": "xchng",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "adb-wifi": "adb devices | grep -q 'device$' && adb tcpip 5555 && adb shell ip route | awk '{print $9}' | xargs -I {} adb connect {} || echo 'No device found via USB'",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "install-all": "yarn install && yarn pod",
    "pod": "cd ios && pod install && cd ..",
    "clean": "yarn clean-ios && yarn clean-android && yarn clean-node && watchman watch-del-all && rm -rf /tmp/metro-*",
    "clean-android": "rm -rf android/.gradle android/.idea android/build android/app/build",
    "clean-ios": "cd ios && pod deintegrate && cd .. && pod cache clean --all && rm -rf ios/build ios/Podfile.lock",
    "clean-node": "rm -rf node_modules yarn.lock",
    "clean-cache": "yarn cache clean --force",
    "prebuild": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "mac-build": "cd android && ./gradlew assembleDebug && cd ..",
    "aab": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew bundleRelease && cd ..",
    "debug-aab": "react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/debug/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew bundleDebug && cd ..",
    "mobile": "~/Library/Android/sdk/tools/emulator -avd Pixel_5_API_31",
    "postinstall": "patch-package && yarn pod",
    "make-release": "cd android && ./gradlew assembleRelease && cd ..",
    "release-build": "npx react-native build-android --mode=release",
    "show-android-menu": "adb shell input keyevent 82",
    "generate-aab": "npx react-native build-android --mode=release"
  },
